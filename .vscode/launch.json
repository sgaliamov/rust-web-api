{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'rust-web-api'",
      "cargo": {
        "args": ["build", "--bin=rust-web-api", "--package=rust-web-api"],
        "filter": {
          "name": "rust-web-api",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'rust-web-api'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=rust-web-api",
          "--package=rust-web-api"
        ],
        "filter": {
          "name": "rust-web-api",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}
